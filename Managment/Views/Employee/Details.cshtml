@model Employee

<h1> Details</h1>

<table class="table table-bordered">
    <tr>
        <td><label asp-for="EmployeeId" ></label> </td>
        <td>@Model.EmployeeId</td>

    </tr>
    <tr>
        <td><label asp-for="Name"></label> </td>
        <td>@Model.Name</td>

    </tr>
     <tr>
        <td><label asp-for="HDate" ></label> </td>
        <td>@Model.HDate</td>

    </tr>
    <tr>
        <td><label asp-for="Email"></label> </td>
        <td>@Model.Email</td>

    </tr>
    <tr>
        <td><label asp-for="Salary"></label> </td>
        <td>@Model.Salary</td>

    </tr>
    <tr>
        <td><label asp-for="City"></label> </td>
        <td>@Model.City</td>

    </tr>
</table>
